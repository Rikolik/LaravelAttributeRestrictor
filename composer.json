{
    "name": "enriko/laravel-attribute-restrictor",
    "description": "For Laravel. Easy way to restrict access to specified attributes in a Model",
    "keywords": [
        "laravel",
        "permission",
        "permissions",
        "roles",
        "security",
        "cast",
        "casting"
    ],
    "autoload": {
        "psr-4": {
            "Enriko\\LaravelAttributeRestrictor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Enriko\\LaravelAttributeRestrictor\\LaravelAttributeRestrictorServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Henrique Lopes Ciecielski",
            "email": "henriquelc000@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9"
    }
}
